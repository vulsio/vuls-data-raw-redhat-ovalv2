	"metadata": {
		"title": "kernel: RDMA: Verify port when creating flow rule (low)",
		"reference": [
			{
				"ref_id": "CVE-2021-47265",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2021-47265",
