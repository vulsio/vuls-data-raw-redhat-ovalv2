	"metadata": {
		"title": "kernel: vmxnet3: Fix missing reserved tailroom (moderate)",
		"reference": [
			{
				"ref_id": "CVE-2024-27026",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2024-27026",
