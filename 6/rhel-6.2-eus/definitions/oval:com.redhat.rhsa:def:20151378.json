  "metadata": {
    "title": "RHSA-2015:1378: hivex security and bug fix update (Moderate)",
    "reference": [
      {
        "ref_id": "RHSA-2015:1378",
        "ref_url": "https://access.redhat.com/errata/RHSA-2015:1378",
        "source": "RHSA"
