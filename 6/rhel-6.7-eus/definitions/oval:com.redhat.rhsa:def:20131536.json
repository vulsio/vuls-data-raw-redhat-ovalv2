  "metadata": {
    "title": "RHSA-2013:1536: libguestfs security, bug fix, and enhancement update (Moderate)",
    "reference": [
      {
        "ref_id": "RHSA-2013:1536",
        "ref_url": "https://access.redhat.com/errata/RHSA-2013:1536",
