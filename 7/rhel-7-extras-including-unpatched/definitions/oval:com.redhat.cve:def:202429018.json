	"metadata": {
		"title": "moby: external DNS requests from 'internal' networks could lead to data exfiltration (moderate)",
		"reference": [
			{
				"ref_id": "CVE-2024-29018",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2024-29018",
