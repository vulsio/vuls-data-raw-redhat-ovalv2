	"metadata": {
		"title": "kernel: powerpc/fixmap: Fix VM debug warning on unmap (low)",
		"reference": [
			{
				"ref_id": "CVE-2021-47623",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2021-47623",
