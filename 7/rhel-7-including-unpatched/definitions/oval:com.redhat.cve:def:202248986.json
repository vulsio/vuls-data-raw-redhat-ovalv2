	"metadata": {
		"title": "kernel: mm/gup: fix gup_pud_range() for dax (moderate)",
		"reference": [
			{
				"ref_id": "CVE-2022-48986",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2022-48986",
				"source": "CVE"
			}
