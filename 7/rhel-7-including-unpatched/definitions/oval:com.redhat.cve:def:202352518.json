	"metadata": {
		"title": "kernel: Bluetooth: hci_codec: Fix leaking content of local_codecs (moderate)",
		"reference": [
			{
				"ref_id": "CVE-2023-52518",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2023-52518",
