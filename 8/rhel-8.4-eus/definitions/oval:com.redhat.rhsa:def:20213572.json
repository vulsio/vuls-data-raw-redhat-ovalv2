  "metadata": {
    "title": "RHSA-2021:3572: nss and nspr security, bug fix, and enhancement update (Moderate)",
    "reference": [
      {
        "ref_id": "RHSA-2021:3572",
        "ref_url": "https://access.redhat.com/errata/RHSA-2021:3572",
