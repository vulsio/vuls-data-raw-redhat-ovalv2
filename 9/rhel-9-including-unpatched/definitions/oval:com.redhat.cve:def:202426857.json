	"metadata": {
		"title": "kernel: geneve: make sure to pull inner header in geneve_rx() (moderate)",
		"reference": [
			{
				"ref_id": "CVE-2024-26857",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2024-26857",
