	"metadata": {
		"title": "kernel: net: hns3: fix kernel crash when 1588 is received on HIP08 devices (moderate)",
		"reference": [
			{
				"ref_id": "CVE-2024-26881",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2024-26881",
