	"metadata": {
		"title": "kernel: iommu/vt-d: Don&#39;t issue ATS Invalidation request when device is disconnected (low)",
		"reference": [
			{
				"ref_id": "CVE-2024-26891",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2024-26891",
				"source": "CVE"
			}
