	"metadata": {
		"title": "kernel: drm/amdgpu: fix deadlock while reading mqd from debugfs (moderate)",
		"reference": [
			{
				"ref_id": "CVE-2024-35795",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2024-35795",
				"source": "CVE"
			}
