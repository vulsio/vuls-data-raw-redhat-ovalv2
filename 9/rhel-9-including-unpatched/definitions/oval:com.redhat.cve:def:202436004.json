	"metadata": {
		"title": "kernel: i40e: Do not use WQ_MEM_RECLAIM flag for workqueue (moderate)",
		"reference": [
			{
				"ref_id": "CVE-2024-36004",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2024-36004",
