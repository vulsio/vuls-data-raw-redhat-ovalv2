	"metadata": {
		"title": "kernel: ppp: do not assume bh is held in ppp_channel_bridge_input() (moderate)",
		"reference": [
			{
				"ref_id": "CVE-2024-49946",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2024-49946",
				"source": "CVE"
			}
