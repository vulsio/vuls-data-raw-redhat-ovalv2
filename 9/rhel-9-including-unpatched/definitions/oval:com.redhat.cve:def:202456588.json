	"metadata": {
		"title": "kernel: scsi: hisi_sas: Create all dump files during debugfs initialization (low)",
		"reference": [
			{
				"ref_id": "CVE-2024-56588",
				"ref_url": "https://access.redhat.com/security/cve/CVE-2024-56588",
